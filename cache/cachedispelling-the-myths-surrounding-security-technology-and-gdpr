<script>
    (function($, win) {
    var numOfPageLinks = 0,
        maxPaginationLinks = 3,
        maxNumberOfLinksWithoutMore, pageLinksObj, activePageIndex, lastPageLink, firstPageLink, backupPageLinksObj, isMobile,
        selectors = {
            parent: ".pagination"
        },
        fnNumOfPages = function() {
            return $(selectors.parent).children('a, span.pacurrent');
        },
        calc = function() {
            var i, plusMinus, displayStartFrom, displayEndAt, iterateTo, borderFirstLink = false, borderLastLink = false;
            if (numOfPageLinks > maxNumberOfLinksWithoutMore) {
                if (activePageIndex < maxPaginationLinks) {
                    displayStartFrom = 1;
                    displayEndAt = maxPaginationLinks+1;
                    borderFirstLink = false;
                    borderLastLink = true;
                } else if ((numOfPageLinks - activePageIndex) < maxPaginationLinks) {
                    displayStartFrom = numOfPageLinks - maxPaginationLinks;
                    displayEndAt = numOfPageLinks;
                    borderFirstLink = true;
                    borderLastLink = false;
                } else {
                    plusMinus = Math.floor(maxPaginationLinks / 2);
                    displayStartFrom = activePageIndex - plusMinus;
                    displayEndAt = activePageIndex + plusMinus;



                    borderFirstLink = true;
                    borderLastLink = true;

                    if(isMobile){
                        if(activePageIndex === maxPaginationLinks){
                            borderFirstLink = false;
                        } 

                        if((numOfPageLinks - (activePageIndex - 1)) === maxPaginationLinks){
                            borderLastLink = false;
                        }
                    } else {
                        if(activePageIndex === maxPaginationLinks - 1){
                            borderFirstLink = false;
                        } 

                        if((numOfPageLinks - activePageIndex + 1) === maxPaginationLinks){
                            borderLastLink = false;
                        }
                    }

                    
                }
                iterateTo = numOfPageLinks - 2;
                console.log(pageLinksObj[iterateTo]);
                //Logic
                for (i = 1; i <= iterateTo; i++) {
                    console.log("before" + i);
                    if (i >= displayStartFrom-1 && i <= displayEndAt-1) {
                        continue;
                    }
                    console.log("after" + i);
                    $(pageLinksObj[i]).hide();
                }

                //add border
                if (borderLastLink) {
                        $(lastPageLink).addClass("borderLeft");
                } else {
                        $(lastPageLink).removeClass("borderLeft");
                }

                if (borderFirstLink){
                    $(firstPageLink).addClass("borderRight");
                } else {
                    $(firstPageLink).removeClass("borderRight");
                }
            }
        },
        init = function() {
            isMobile = (window.innerWidth <= 768);
            maxPaginationLinks = isMobile ? 3 : 11;
            maxNumberOfLinksWithoutMore = maxPaginationLinks + 1;
            pageLinksObj = fnNumOfPages();
            backupPageLinksObj = pageLinksObj;
            numOfPageLinks = pageLinksObj.length;
            activePageIndex = $(".pagination .pacurrent").index();
            firstPageLink = $(pageLinksObj[0]);
            lastPageLink = $(pageLinksObj[numOfPageLinks - 1]);
            calc();
        },
        makeVisible = function() {
            $(selectors.parent).css("visibility", "visible");
        },
        delayDisplay = function() {
            setTimeout(makeVisible, 500);
        },
        reset = function() {
            var i = 0;
            pageLinksObj = backupPageLinksObj;
            for (i = 0; i < pageLinksObj.length; i++) {
                $(pageLinksObj[i]).show();
            }
            init();
        };
    $(function() {
        init();
        delayDisplay();
    });
    $(win).on('resize', reset);
})(jQuery.noConflict(), window);
//End of Praveen Code for Pagination

//categories sub menu
(function($, win){
    
    var selectors = {
        onBody: "body",
        menuButton: ".sub-navigation .drop-down a.sub-nav-main",
        secMenu: ".sub-navigation .categories-menu"
    },

    closeMenu = function(){
      $(selectors.secMenu).removeClass("open").addClass("close");
      $(selectors.menuButton).removeClass("open").addClass("close");
    },

    toggleMenu = function(){
      $(selectors.secMenu).toggleClass("open close");
      $(selectors.menuButton).toggleClass("open close");
    };

    //on ready
    $(function(){

      $(selectors.onBody).on('click', function(e){
        closeMenu();
      });
      $(selectors.secMenu).on('click', function(e){
        e.stopPropagation();
      });
      $(selectors.menuButton).on("click", function(e){
        e.stopPropagation();
        toggleMenu();
      });

    });

})(jQuery.noConflict(), window);


(function($) {
    if ($('.pacurrent').next().hasClass('next') == true) {
        $('.next').addClass('last');
    }
    if ($('.pacurrent').prev().hasClass('prev') == true) {
        $('.prev').addClass('first');
    }
    //To identify homepage.
    if (window.location.pathname == "/" || window.location.pathname == "/index.html") {
        $('body').addClass('homepage');
    }
    //To identify the last page or 10th page.
    if (window.location.pathname == "/index10.html") {
        $('body').addClass('lastpage');
    }
    $('.nav__items label[data-icon="search"]').click(function() {
        var inputfield = $(this).parent().find('#st-search-input-mainnav').val();
        if (inputfield.length != 0) {
            $(this).parent().submit();
        }
    });
    $(".primary-nav__level-one .burger-icon").on("click", function(e) {
        if ($(this).hasClass('active')) {
            closeNav();
        }
        $(this).toggleClass('active');
        $('.menu-items').toggleClass('mobile-nav');
        $('.nav-content').toggleClass('mobile-nav');
        setMobileNavScroll();
    })
    $(".nav-overlay").hide();
    $('.utility-nav__inner-wrapper.inner-wrapper').addClass("utility-overlay");
    $('body').on('click', function(e) {
        if ($('.utility-nav').find('.tab__radio').first().prop('checked')) {
            // if user clicked outside of utility nav close it
            if ($(e.target).closest($('.utility-nav')).length < 1) {
                $('.utility-nav').find('.tab__radio:checked').first().prop('checked', false);
                $('.utility-nav').find('.tab__close-tab .tab__radio:checked').first().prop('checked', true);
            }
        }
        if ($('.language-selector').find('.tab__radio').first().prop('checked')) {
            // if user clicked outside of utility nav close it
            if ($(e.target).closest($('.language-selector')).length < 1) {
                $('.language-selector').find('.tab__radio:checked').first().prop('checked', false);
                $('.language-selector').find('.tab__close-tab .tab__radio:checked').first().prop('checked', true);
            }
        }
    });

    function setSize() {
        var parentWidth = $('.nav-content--2').width();
        parentWidth = parentWidth / 4;
        parentWidth = parentWidth + 7;
        $('.nav-content--2 > ul > li').width(parentWidth);
    }

    function windowWidth() {
        var windowWidth = $(window).width();
        if (windowWidth < 800) {
            $('.controls').hide();
            $('.four-column > li').css('width', '100%');
        } else {
            $('.controls').show();
        }
    }
    //setSize();
   windowWidth();
    $(window).resize(function() {
        //setSize();
        windowWidth();
    })
    var currentVal = $('.language-selector').find("li.ui-list__item.select").attr("lang");
    var labelName = $('.language-selector').find("li.ui-list__item.select").text();
    $('.language-selector').find("label.tab__label").text(labelName);
    $('.language-selector').find("li.ui-list__item").on('click', function(e) {
        var newVal = $(e.target).attr("lang");
        var selectLangVal = newVal;
        newVal = "https://" + window.location.host + newVal;
        if (newVal !== currentVal) {
            window.location.href = newVal;
        }
    });
    $('.controls .previous').on('click', function(e) {
        $('.controls .next').removeClass('is-disabled');
        var itemWidth = parseInt($('.four-column > li').css('width'));
        var leftVal = parseInt($('.four-column').css('left'));
        var finalVal = leftVal + itemWidth;
        $('.four-column').css('left', finalVal);
        //console.log(finalVal);
        if (finalVal > 0 || finalVal == 0) {
            $(this).addClass('is-disabled');
        } else {
            $('.controls .next').removeClass('is-disabled');
        }
    });
    $('.controls .next').on('click', function(e) {
        $('.controls .previous').removeClass('is-disabled');
        var itemWidth = parseInt($('.four-column > li').css('width'));
        var leftVal = parseInt($('.four-column').css('left'));
        var finalVal = leftVal - itemWidth;
        $('.four-column').css('left', finalVal);
        var itemLength = $('.four-column > li').length;
        var totalVal = itemWidth * (itemLength - 4);
        if (totalVal == -(finalVal)) {
            $(this).addClass('is-disabled');
        } else {
            $('.controls .previous').removeClass('is-disabled');
        }
    });

    function setMobileNavScroll() {
		var width = $(window).width();
		var height = $(window).height();
		height = height - 135;
		height = height + 'px';
		if (width <= 800) {
			$('.nav-content div > ul').css('height', height);
			if ($(".primary-nav__level-one .burger-icon").hasClass('active')) {
			$('.menu-items').addClass('mobile-nav');
			$('.nav-content').addClass('mobile-nav');
		} else {
		if($('.main-nav-item.search.selected').length<1){
			closeNav();	
		}
		}
		} else {
			$('.nav-content div ul').removeAttr('style');
			$('.menu-items').removeClass('mobile-nav');
			$('.nav-content').removeClass('mobile-nav');
		}
	}
    $(window).resize(function() {
        setMobileNavScroll();
    })
    $(".primary-nav__level-one .nav__items .tab__label").on("click", function(e) {
        //   count += 1;
        if ($(this).hasClass('active')) { // already been clicked
            $(this).removeClass('active'); // add one
            closeNav();
        } else { // first click
            $(".primary-nav__level-one .nav__items .tab__label").removeClass('active');
            $(this).addClass('active'); // initialize the count
            $(".nav-overlay").show();
        }
    })
    $(".nav-overlay").on("click", function(e) {
        //   count += 1;
        closeNav();
    })
    $('.tabs__single-tab.searchBox input').on("focus", function(e) {
        closeNav();
    });

    function closeNav() {
        window.setTimeout(function() {
            $("#resetNav").trigger("click");
            $(".primary-nav__level-one .nav__items .tab__label").removeClass('active');
            $('.utility-nav__inner-wrapper.inner-wrapper').removeClass("utility-overlay");
            $(".nav-overlay").hide();
        }, 100);
    }
    $('.utility-nav__level-one .mkt-utility-main').on('click', function(e) {
        $("#resetNav").trigger("click");
        $(".primary-nav__level-one .nav__items .tab__label").removeClass('active');
        $(".nav-overlay").hide();
    });
    var navTop = $('.hero-intro-region').offset().top;
    var introHeight = $('.hero-intro-region').outerHeight();
    var totalHeight = 110;
    var navHeight;
    var mousewheelevt = (/Firefox/i.test(navigator.userAgent)) ? "DOMMouseScroll" : "mousewheel"; //FF doesn't recognize mousewheel as of FF3.x
    var headerHeight = $('.hero-intro-region').outerHeight();
    if (headerHeight < 289) $('.sticky-widgets').addClass('gray');
    $('.sticky-visible').on('click', function(e) {
        e.stopPropagation();
        $(this).parent().parent().find('.focus-sticky-div').hide();
        var hasClass = $('.primary-nav__outer-wrapper').hasClass("floater");
        if (hasClass) {
            navHeight = $('.primary-nav__outer-wrapper').outerHeight();
        } else {
            navHeight = $('.global-header').outerHeight();
        }
        $('.focus-sticky-div.chat').css('top', navHeight);
        $(this).parent().find('.social-overlay').show();
        $(this).parent().children('.focus-sticky-div').show();
    });
    $('.focus-sticky-div .close').on('click', function() {
        $('.focus-sticky-div').hide();
        $('.social-overlay').hide();
    });
    $('.focus-sticky-div.social').on('click', function() {
        $('.focus-sticky-div').hide();
    });
    $('.social-overlay').on('click', function() {
        $('.focus-sticky-div').hide();
        $('.social-overlay').hide();
    });
    $(window).on("scroll", function() {
        var stickyWidget = $('.sticky-widgets'),
            scrollSticky = $(window).scrollTop();
        if (scrollSticky >= totalHeight) {
            stickyWidget.addClass('gray');
            $('.focus-sticky-div.chat').css('top', headerHeight);
        } else {
            stickyWidget.removeClass('gray');
            $('.focus-sticky-div.chat').css('top', megaNavHeight);
            var headerHeight = $('.hero-intro-region').outerHeight();
            if (headerHeight < 289) $('.sticky-widgets').addClass('gray');
        }
        var floaterClass = $('.primary-nav__outer-wrapper').hasClass("floater"),
            megaNavHeight = $('.primary-nav__outer-wrapper').outerHeight(),
            headerHeight = $('.global-header').outerHeight();
        if (scrollSticky >= totalHeight) {
            $('.focus-sticky-div.chat').css('top', 0);
        } else {
            $('.focus-sticky-div.chat').css('top', headerHeight);
        }
    });
    //Mobile Search
    $('.swiftype-header-search').find("input").on({
        "click": function() {
            if (!$(this).hasClass("mobile-search-input")) {
                $(this).parents("nav").find(".nav-overlay").show();
                $(this).parents(".searchBox").css("z-index", 999);
                $(this).parents(".searchBox").addClass("no-hover");
            }
        },
        "input": function() {
            $(this).parent().find('form').submit();
        }
    });
    $('.swiftype-header-search').find(".mobile-search-overlay").on("click", function() {
        $(this).prev().find("label").trigger("click");
    });
    $(".nav-overlay").on("click", function() {
        $(this).hide();
        $(this).parents("nav").find(".searchBox").removeClass("no-hover").attr("style", "");
    });
    $('.swiftype-header-search').find("label").click(function(e) {
        if ($(this).next().find(".st-search-input.mobile-search-input").length > 0) {
            $(this).next().find(".st-search-input.mobile-search-input").toggleClass("active");
            if ($(this).next().find(".st-search-input.mobile-search-input").hasClass("active")) {
                $(this).next().find(".st-search-input.mobile-search-input").animate({
                    "left": "0"
                }, function() {
                    $(this).focus();
                })
                $(this).parent().next().show();
            } else {
                $(this).next().find(".st-search-input.mobile-search-input").animate({
                    "left": "100%"
                })
                $(this).parent().next().hide();
            }
        } else {
            if ($(this).parent().find('input[type="search"]').val() != "") {
                redirectToSearchResults();
            }
        }
    });
})(jQuery.noConflict());
/*New Navigation Mar-2019*/
(function(window){
  var $=window.jQuery;
  if($('.main-nav-container').hasClass("main-nav-container")){
      var lowResCTA=$('.main-nav-container').find(".main-nav-featured .orange-cta a.button").clone(true);
      $('.main-nav-container').find(".main-util").prepend(
        $("<div/>").addClass('low-res-cta').append(lowResCTA)
      );
        
      
      $('.main-nav-container').find(".main-nav-item > label,.language > label").each(function(){
        $(this).on("click",function(){
          $('.main-nav-container').find(".main-nav-item.selected,.language.selected").not($(this).parent()).each(function(){
            $(this).removeClass('selected');
            $(this).find(".tab-content").hide();
          });

		   if(!$(this).parent().hasClass('orange-cta')){
             $(this).parent().toggleClass('selected');
          }
		  
          if($(this).parent().hasClass('selected')){
            $(this).parent().find(".tab-content").show();
          }
          else
            $(this).parent().find(".tab-content").hide();
        })
      });
     
      $('.main-nav-container').find(".main-nav-overlay").each(function(){
        $(this).on("click",function(){
          $('.main-nav-container').find(".selected").removeClass('selected');
          $('.main-nav-container').find(".tab-content").hide();
        });
      });

      $('.main-nav-container').find(".main-nav-featured .tab-content span.close--nav").each(function(){
        $(this).on("click",function(){
          $('.main-nav-container').find(".main-nav-overlay").trigger("click");
        });
      });


      $("body").on("click",function(e){
        if($(e.target).parents(".main-nav-container").length<1 && !$(e.target).hasClass('.main-nav-container')){
          $('.main-nav-container').find(".main-nav-item.selected,.language.selected").each(function(){
            $(this).removeClass('selected');
            $(this).find(".tab-content").hide();
          });
        }
      })
      if($('.main-nav-container').find("input:checked").length>0){
        $('.main-nav-container').find("input:checked").each(function(){
          $(this).parent().addClass('selected');
        });
      }
    }
})(window);
jQuery('body').on('click',function(){
  if(jQuery('.focus-sticky-div.social').css('display') !== 'none'){
       jQuery('.focus-sticky-div').hide();
    }
});
jQuery('body').on('click', function(e) {
    // check if lang sel tab is currently open
    if (jQuery('.main-util .language').find('.tab__radio').first().prop('checked')) {
        // if user clicked outside of lang sel tab close it
        if (jQuery(e.target).closest(jQuery('.main-util .language')).length < 1) {
            jQuery('.main-util .language').find('.tab__radio:checked').first().prop('checked', false);
            jQuery('.main-util .language').find('.tab__close-tab .tab__radio:checked').first().prop('checked', true);
        }
    }
    if (jQuery('.main-util .language').find("#language").length > 0 && jQuery('.main-util .language').find("#language").is(":checked") && jQuery(e.target).closest(jQuery('.main-util .language')).length < 1) {
        jQuery('.main-util .language').find("#language")[0].checked = false;
        jQuery('.main-util .language').removeClass('selected');
        jQuery('.main-util .language').find(".tab-content").removeAttr('style')
    }
});
/*End of New Navigation Mar-2019*/
